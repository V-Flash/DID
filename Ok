<!-- Modal 1 -->
<div class="modal fade" id="exampleModal1" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <!-- ... modal content ... -->
  <div class="modal-body">
    <div class="ok"></div>
  </div>
  <!-- ... modal content ... -->
</div>

<!-- Button trigger modal 1 -->
<button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#exampleModal1">
  Launch demo modal 1
</button>

<!-- Modal 2 -->
<div class="modal fade" id="exampleModal2" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <!-- ... modal content ... -->
  <div class="modal-body">
    <div class="ok"></div>
  </div>
  <!-- ... modal content ... -->
</div>

<!-- Button trigger modal 2 -->
<button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#exampleModal2">
  Launch demo modal 2
</button>

<script>
  var names = ["Naruto", "luffy", "zoro", "hitachi"];

  // Function to populate the "ok" div in modals
  function populateModalOkDiv(modalId, namesArray) {
    var okDiv = document.querySelector(`${modalId} .modal-body .ok`);
    okDiv.innerHTML = namesArray.join(', ');
  }

  // Call the function to populate the "ok" div for each modal
  populateModalOkDiv('#exampleModal1', names);
  populateModalOkDiv('#exampleModal2', names);
</script>
