<!DOCTYPE html>
<html>
<head>
    <!-- Your other HTML head content here -->
</head>
<body>
    <!-- Your other HTML body content here -->

    <!-- Container for displaying the div elements -->
    <div id="outputContainer"></div>

    <!-- Your other HTML body content here -->

    <script>
        // Array of names
        var names = ["Alice", "Bob", "Charlie", "David", "Eve"];

        // Function to create and append div elements with names
        function createDivElementsWithNames(names) {
            names.forEach(function(name) {
                var div = document.createElement("div");
                div.textContent = 'hello ' + name;
                document.getElementById("outputContainer").appendChild(div);
            });
        }

        // Call the function to create and append div elements
        createDivElementsWithNames(names);
    </script>
</body>
</html>
