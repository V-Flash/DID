const header = document.querySelector('.header');

            if (header && header.classList.contains('fixed')) {
                // Header has 'fixed' class, execute script for fixed header
                scrollToSectionWithFixedHeader(sectionId);
            } else {
                // Header does not have 'fixed' class, execute script for non-fixed header
                scrollToSectionWithoutFixedHeader(sectionId);
            }
