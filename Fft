// Get the element where you want to insert the checkbox list
var listContainer = document.getElementById("listContainer"); // Replace "listContainer" with the actual ID of the container element

// Construct the HTML for the checkbox list
var checkboxListHTML = `
  <li>
    <input type="checkbox" id="option"> <label for="option"> Cables</label>
    <ul>
      <li><label><input type="checkbox" class="subOption"> LV</label></li>
      <li><label><input type="checkbox" class="subOption"> HV</label></li>
      <li><label><input type="checkbox" class="subOption"> EHV</label></li>
    </ul>
  </li>
`;

// Set the HTML content of the container element
listContainer.innerHTML = checkboxListHTML;
