jQuery(document).ready(function($) {
  var intestIndex = 1; // Initialize the intest index

  for (var i = 1; i <= anchorElements.length; i++) {
    var intest = $('#intest' + intestIndex);
    var test = $('#test' + i); // Use i for test element
    
    $(intest).on('change keyup paste', function () {
      $(test).attr("data-quantity", $(intest).val());
    });

    intestIndex += 2; // Increment the intest index by 2 for the next iteration
  }
});
