
var anchorElements = document.querySelectorAll(".ut_single_cart_btn");


for (var i = 0; i < anchorElements.length; i++) {
    anchorElements[i].id = "test" + (i + 1);
}


var inputvalElements = document.querySelectorAll(".wqpmb_input_text");


for (var i = 0; i < inputvalElements.length; i++) {
    inputvalElements[i].id = "intest" + (i + 1);
}
