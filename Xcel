 jQuery( document ).ready(function($) {
    var anchorElements = document.querySelectorAll(".ut_single_cart_btn");
   
     for(i = 1; i <= anchorElements.length; i++){
      
      $('#intest1').on('change keyup paste', function () {
       
        $('#test1').attr("data-quantity", $('#intest1').val())
      });
    }
});
