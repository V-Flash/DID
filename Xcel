<!DOCTYPE html>
<html>
<head>
    <title>Generate Anchor Elements</title>
</head>
<body>
    <script>
        // Define the number of anchor elements you want to create
        var numberOfElements = 5; // You can change this to the desired number

        // Loop to create anchor elements with IDs
        for (var i = 1; i <= numberOfElements; i++) {
            // Create a new anchor element
            var anchor = document.createElement("a");

            // Set the ID attribute with "test" followed by the current loop index
            anchor.id = "test" + i;

            // Set the href attribute (for example, to a URL)
            anchor.href = "https://example.com"; // Change this to your desired URL

            // Set the anchor text
            anchor.textContent = "Link " + i;

            // Append the anchor element to the body
            document.body.appendChild(anchor);
        }
    </script>
</body>
</html>
