    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">

    <!-- Taggd CSS -->
    <link rel="stylesheet" href="path/to/taggd.css">

    <!-- jQuery -->
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"></script>

    <!-- Bootstrap JS and Popper.js -->
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>

    <!-- Taggd JS -->
    <script src="path/to/taggd.min.js"></script>
</head>
<body>

<div class="container mt-5">
    <h1 class="text-center">Interactive Image Map with Taggd</h1>

    <div class="text-center mt-3">
        <img id="taggd-image" src="path/to/your/image.jpg" alt="Interactive Image">
    </div>
</div>

<script>
    $(document).ready(function () {
        // Initialize Taggd
        var image = $('#taggd-image');
        var data = [
            { x: 0.2, y: 0.4, text: 'Tag 1' },
            { x: 0.5, y: 0.7, text: 'Tag 2' },
            // Add more tags as needed
        ];

        image.taggd(data);
    });
</script>

