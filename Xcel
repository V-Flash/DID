jQuery(document).ready(function($) {
  // Get all elements with the class .ut_single_cart_btn
  var anchorElements = document.querySelectorAll(".ut_single_cart_btn");

  // Loop through all the elements
  anchorElements.forEach(function(element, index) {
    // Construct the IDs for intest and test
    var intest = $('#intest' + (index + 1));
    var test = $('#test' + (index + 1));

    // Attach event listener to the intest input field
    $(intest).on('change keyup paste', function() {
      // Update the data-quantity attribute of the associated test element
      $(test).attr("data-quantity", $(intest).val());
    });
  });
});
