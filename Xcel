<span class="thumb-prev slick-arrow" style=""></span>

<!-- Include Bootstrap JS (for modal) -->
<!-- Add your Bootstrap CDN link here -->

<script>
  $(document).ready(function() {
    $(".show-btn").on("click", function() {
      // Auto click the span element two times
      $(".thumb-prev").click();
      $(".thumb-prev").click();
    });
  });
</script>

