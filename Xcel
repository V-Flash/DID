jQuery(document).ready(function($) {
  // Loop through odd #intest IDs
  for (var i = 1; i <= 9; i += 2) {
    var $intest = $('#intest' + i);
    var $test = $('#test' + (i + 1)); // Assuming the corresponding #test has the next even number
    
    // Add an event listener to the current #intest input element
    $intest.on('change keyup paste', function () {
      // Update the data-quantity attribute of the corresponding #test element
      $test.attr("data-quantity", $intest.val());
    });
  }
});
