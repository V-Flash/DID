<!-- Script for jQuery 3.3.1 -->
<script>
  var jq331 = jQuery.noConflict(true);

  jq331(document).ready(function($) {
    $('#modal1').modal('show');
    
    $('#modal1').on('hidden.bs.modal', function () {
      $('#modal2').modal('show');
    });
  });
</script>

